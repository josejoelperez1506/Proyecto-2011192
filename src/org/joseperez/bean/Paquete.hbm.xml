<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.joseperez.bean.Paquete" table="PAQUETE">

		<id name="idPaquete" type="java.lang.Integer">
			<column name="IDPAQUETE" />
			<generator class="sequence-identity">
				<param name="sequence">PAQUETE_SEQ</param>
			</generator>
		</id>

		<property name="receptor" type="java.lang.String">
			<column name="RECEPTOR" />
		</property>
		<property name="emisor" type="java.lang.String">
			<column name="EMISOR" />
		</property>
		<property name="fechaEntrega" type="java.lang.String">
			<column name="FECHAENTREGA" />
		</property>
		<property name="fechaEmision" type="java.lang.String">
			<column name="FECHAEMISION" />
		</property>

	</class>
</hibernate-mapping>